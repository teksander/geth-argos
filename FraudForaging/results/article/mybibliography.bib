	note = {Series Title: Springer Tracts in Advanced Robotics},
	pages = {417--430},
	file = {Hoff et al. - 2013 - Distributed Colony-Level Algorithm Switching for R.pdf:/home/eksander/Zotero/storage/6PK8CEQV/Hoff et al. - 2013 - Distributed Colony-Level Algorithm Switching for R.pdf:application/pdf},
