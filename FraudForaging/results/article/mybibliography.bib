	editor = {Dorigo, Marco and Birattari, Mauro and Blum, Christian and Christensen, Anders L. and Reina, Andreagiovanni and Trianni, Vito},
	year = {2018},
	pages = {135--149},
	file = {Springer Full Text PDF:/home/eksander/Zotero/storage/3MWZCJJT/Font Llenas et al. - 2018 - Quality-Sensitive Foraging by a Robot Swarm Throug.pdf:application/pdf},
}

@article{houston_general_1985,
	title = {A general theory of central place foraging for single-prey loaders},
